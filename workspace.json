{
  "version": 2,
  "projects": {
    "app": {
      "root": "apps/app",
      "projectType": "application",
      "sourceRoot": "apps/app/src",
      "targets": {
        "build": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "build"
          }
        },
        "serve": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "dev"
          }
        },
        "add": {
          "executor": "@nxext/sveltekit:add"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/app/**/*.{ts,svelte,spec.ts}"]
          }
        }
      },
      "tags": []
    },
    "proxy": {
      "root": "libs/proxy",
      "projectType": "library",
      "sourceRoot": "libs/proxy",
      "targets": {
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      },
      "tags": []
    },
    "server": {
      "root": "apps/server",
      "projectType": "application",
      "sourceRoot": "apps/server",
      "implicitDependencies": ["app"],
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["npm run copy-go-server", "npx nx build-app server"]
          }
        },
        "build-app": {
          "executor": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/server",
            "main": "dist/apps/app/main.go"
          }
        },
        "serve": {
          "executor": "@nx-go/nx-go:serve",
          "options": {
            "cmd": "gow",
            "cwd": ".",
            "main": "apps/server/main.go"
          }
        },
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      },
      "tags": []
    },
    "swagger-proxy": {
      "root": "apps/swagger-proxy",
      "projectType": "application",
      "sourceRoot": "apps/swagger-proxy",
      "targets": {
        "build": {
          "executor": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/swagger-proxy",
            "main": "apps/swagger-proxy/main.go"
          }
        },
        "serve": {
          "executor": "@nx-go/nx-go:serve",
          "options": {
            "main": "apps/swagger-proxy/main.go"
          }
        },
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      },
      "tags": []
    }
  }
}

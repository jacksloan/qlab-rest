{
  "version": 2,
  "projects": {
    "goodwin": {
      "root": "apps/goodwin",
      "projectType": "application",
      "sourceRoot": "apps/goodwin/src",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "shx mkdir dist/apps/goodwin",
              "npx nx build-app goodwin",
              "npm run copy-proxy -- dist/apps/goodwin/main.go"
            ]
          }
        },
        "build-app": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "build"
          }
        },
        "serve": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "dev"
          }
        },
        "add": {
          "executor": "@nxext/sveltekit:add"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/goodwin/**/*.{ts,svelte,spec.ts}"]
          }
        }
      },
      "tags": []
    },
    "proxy": {
      "root": "libs/proxy",
      "projectType": "library",
      "sourceRoot": "libs/proxy",
      "targets": {
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      },
      "tags": []
    },
    "qlab-rest": {
      "root": "apps/qlab-rest",
      "projectType": "application",
      "sourceRoot": "apps/qlab-rest",
      "targets": {
        "build": {
          "executor": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/qlab-rest",
            "main": "apps/qlab-rest/main.go"
          }
        },
        "serve": {
          "executor": "@nx-go/nx-go:serve",
          "options": {
            "main": "apps/qlab-rest/main.go"
          }
        },
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      },
      "tags": []
    },
    "voice-cmd": {
      "root": "apps/voice-cmd",
      "projectType": "application",
      "sourceRoot": "apps/voice-cmd/src",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "shx mkdir dist/apps/voice-cmd",
              "npx nx build-app voice-cmd",
              "npm run copy-proxy -- dist/apps/voice-cmd/main.go"
            ]
          }
        },
        "build-app": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "build"
          }
        },
        "serve": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "dev"
          }
        },
        "add": {
          "executor": "@nxext/sveltekit:add"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/voice-cmd/**/*.{ts,svelte,spec.ts}"]
          }
        }
      },
      "tags": []
    }
  }
}

{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "scraper",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "specFileName": {
      "type": "string",
      "description": "The name of the spec json file that will be written",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["name"]
}

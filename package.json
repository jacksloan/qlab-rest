    "@nrwl/eslint-plugin-nx": "13.9.4",
    "@nrwl/express": "13.9.4",
    "@nrwl/jest": "13.9.4",
    "@nrwl/linter": "13.9.4",
    "@nrwl/node": "13.9.4",

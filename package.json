{
  "name": "sir-goodwin",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target serve --projects app,server --parallel",
    "start:app": "nx serve app",
    "start:server": "nx serve server",
    "start:swagger": "nx serve swagger-proxy",
    "start:voice-command": "nx run-many --target serve --projects voice-cmd,server --parallel",
    "build": "nx run-many --target build --all --skip-nx-cache",
    "build:app": "nx build app",
    "build:server": "nx build server",
    "build:swagger": "nx build swagger-proxy",
    "package": "pkg .",
    "scraper": "npx nx workspace-generator scraper --dir=apps/swagger-proxy/swagger --no-interactive",
    "codegen": "openapi-generator-cli generate --generator-key openapitools.json",
    "copy-go-server": "shx cp apps/server/main.go dist/apps/app/",
    "run": "./sir-goodwin",
    "bpr": "run-s build package run",
    "test": "nx test",
    "preinstall": "npm run codegen"
  },
  "private": true,
  "dependencies": {
    "@jbs/codegen": "file:./codegen",
    "annyang": "^2.6.1",
    "cors": "^2.8.5",
    "double-metaphone": "^2.0.0",
    "express": "4.17.2",
    "morgan": "^1.10.0",
    "number-to-words": "^1.2.4",
    "open": "^8.4.0",
    "osc": "^2.4.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^13.3.1",
    "@nrwl/cli": "13.9.6",
    "@nrwl/devkit": "13.9.6",
    "@nrwl/eslint-plugin-nx": "13.9.6",
    "@nrwl/express": "13.9.6",
    "@nrwl/jest": "13.9.6",
    "@nrwl/linter": "13.9.6",
    "@nrwl/node": "13.9.6",
    "@nrwl/workspace": "13.9.6",
    "@nx-go/nx-go": "^2.4.0",
    "@nxext/sveltekit": "^13.0.0-next.2",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.181",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "autoprefixer": "^10.4.2",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "jest": "27.2.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "nx": "13.9.6",
    "openapi-types": "^10.0.0",
    "path-parser": "^6.1.0",
    "pkg": "^5.5.2",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "2.6.0",
    "shx": "^0.3.4",
    "svelte": "^3.44.0",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^3.0.12",
    "ts-jest": "27.0.5",
    "typescript": "^4.5.2"
  },
  "bin": "dist/apps/server/main.js",
  "pkg": {
    "targets": [
      "node16-macos-arm64"
    ],
    "assets": [
      "dist/apps/app/**/*",
      "dist/apps/openapi.json",
      "dist/apps/api-docs/**/*"
    ]
  }
}

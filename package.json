{
  "name": "sir-goodwin",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:goodwin": "nx run-many --target serve --projects goodwin,qlab-proxy --parallel",
    "start:qlab-proxy": "nx serve qlab-proxy",
    "start:swagger": "nx serve swagger-proxy",
    "start:voice-cmd": "nx run-many --target serve --projects voice-cmd,server --parallel",
    "build": "shx rm -rf dist && nx run-many --target build --all --skip-nx-cache",
    "build:godwin": "nx build goodwin",
    "build:qlab-proxy": "nx build qlab-proxy",
    "build:swagger": "nx build swagger-proxy",
    "test": "nx test",
    "scraper": "npx nx workspace-generator scraper --dir=apps/swagger-proxy/swagger --no-interactive",
    "codegen": "openapi-generator-cli generate --generator-key openapitools.json",
    "copy-proxy": "shx cp apps/qlab-proxy/main.go"
  },
  "private": true,
  "dependencies": {
    "@jbs/codegen": "file:./codegen",
    "annyang": "^2.6.1",
    "double-metaphone": "^2.0.0",
    "number-to-words": "^1.2.4",
    "osc": "^2.4.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0"
  },

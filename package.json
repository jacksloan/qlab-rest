{
  "name": "sir-goodwin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:client": "npm run codegen && rollup -c",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "start": "npm run build && npm run package && ./sir-goodwin",
    "package": "rm ./sir-goodwin && pkg .",
    "codegen": "npm run codegen && openapi-generator-cli generate -g typescript-fetch -i \"./openapi: '3.0.2'.yml\" -o ./generated",
    "dev": "rollup -c -w",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "bin": "dist/server/main.js",
  "pkg": {
    "targets": [
      "node16-macos-arm64"
    ],
    "assets": [
      "dist/client/**/*"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/annyang": "^2.6.2",
    "@types/cheerio": "^0.22.31",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "openapi-types": "^10.0.0",
    "path-parser": "^6.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "annyang": "^2.6.1",
    "express": "^4.17.3",
    "osc": "^2.4.2",
    "rxjs": "^7.5.5",
    "sirv-cli": "^2.0.0"
  }
}
